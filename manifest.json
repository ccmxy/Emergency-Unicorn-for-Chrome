{
  "manifest_version": 2,
  "name": "Unicorn",
  "description": "changes images and words to unicorn related ones upon icon click",
  "version": "2.0",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Unicorn"
  }
}
